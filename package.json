{
  "name": "vfg-math",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "google-protobuf": "^3.17.3",
    "grpc-web": "1.2.1",
    "jwt-decode": "^3.1.2",
    "katex": "^0.13.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sanitize.css": "^12.0.1",
    "yauk": "^2.4.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "proto": "cd ./src/proto && protoc -I=. --js_out=import_style=commonjs,binary:. --grpc-web_out=import_style=typescript,mode=grpcwebtext:. *.proto",
    "format:write": "prettier --write ./src",
    "format": "prettier --check ./src",
    "docker:build": "docker build . -t smrtrfszm/vfg-math --build-arg BA_URL=https://api.191.verseghy-gimnazium.net"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/katex": "^0.11.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router-dom": "^5.1.7",
    "cross-env": "^7.0.3",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "sass": "^1.34.1",
    "typescript": "^4.1.0"
  }
}
